        </doc>
        <response status="204"/>
        &commonFaults; &getFaults; &postPutFaults;
    </method>

    <method id="executePolicy" name="POST">
        <doc title="Execute Policy">
            <db:para role="shortdesc">
                This method executes a scaling policy.
            </db:para>
            <p xmlns="http://www.w3.org/1999/xhtml">
                This method executes the specified scaling policy. If execution is successful, a response body is
            returned.
            </p>
        </doc>
        <request>
            <representation mediaType="application/json">
                <doc xml:lang="en"/>
